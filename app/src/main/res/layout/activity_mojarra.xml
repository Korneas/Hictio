<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fishes.MojarraActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/mojarra_layout"
        android:backgroundTint="@color/colorPrimaryHictio">

        <TextView
            android:id="@+id/textMojarra"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/colorAccentHictio"
            android:textSize="20dp"
            android:text="Mojarra luminosa" />
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>